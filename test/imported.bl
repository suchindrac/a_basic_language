VARBLOCK <#
avar = 10
bvar = 15
alink: impmod <-> val
print - Printing variables from imported file
print Value of avar is {avar}
print Value of alink is {alink}
#>

PRTBLOCK <#
print Printing from imported file
#>

CHGBLOCK <#
avar = 15
print avar is {avar}
cvar = 2 * avar * avar + bvar + 10
print cvar is {cvar}
#>

MAINBLOCK <#
print Inside MAINBLOCK of imported file
print Calling VARBLOCK
exec VARBLOCK
print Calling PRTBLOCK
exec PRTBLOCK,5
#>